    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/uuid": "^10.0.0",
    "fs-extra": "^11.0.0",
    "openai": "^4.0.0",
    "uuid": "^11.1.0"
  },
  "files": [
    "dist"
  ]
